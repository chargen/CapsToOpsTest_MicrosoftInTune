{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.PNG",
          "**/*.JPG",
          "**/*.GIF",
          "**/*.BMP",
          "**/*_themes/**"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "dest": "_site",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/CapsToOpsTest_EMS/v1/toc.json",
      "ms.locale": "en-us"
    }
  },
  "render": {
    "theme": "hyde",
    "dest": "_output"
  },
  "basepath": "/Intune"
}
